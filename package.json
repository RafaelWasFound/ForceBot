{
  "name": "forcebot",
  "version": "1.0.0",
  "description": "This is the bot that will bring your server to the good side of the force.",
  "main": "dist/bot.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node .",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/bot.ts",
    "test": "jest",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write ./src",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafaelWasFound/ForceBot.git"
  },
  "author": "Rafael",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/RafaelWasFound/ForceBot/issues"
  },
  "homepage": "https://github.com/RafaelWasFound/ForceBot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "discord-api-types": "^0.33.1",
    "discord-hybrid-sharding": "^1.5.3",
    "discord.js": "^13.7.0",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^28.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  }
}
